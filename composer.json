{
  "name": "matthiasmullie/php-api-oauth",
  "type": "project",
  "description": "OAuth API",
  "keywords": [
    "oauth", "api"
  ],
  "homepage": "https://github.com/matthiasmullie/php-api-oauth",
  "license": "MIT",
  "authors": [
    {
      "name": "Matthias Mullie",
      "homepage": "https://www.mullie.eu",
      "email": "php-api-oauth@mullie.eu",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0",
    "matthiasmullie/php-api": "^1.0",
    "symfony/yaml": "^3.2|^4.0",
    "ext-pdo": "*",
    "stampie/stampie": "^1.0",
    "php-http/message": "^1.7",
    "guzzlehttp/psr7": "^1.4",
    "php-http/guzzle6-adapter": "^1.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2.0",
    "phpunit/phpunit": "~6.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "MatthiasMullie\\ApiOauth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MatthiasMullie\\ApiOauth\\Tests\\": "tests/"
    }
  }
}
